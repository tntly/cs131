cd cs131

mkdir worksheet5

cd worksheet5

wget https://raw.githubusercontent.com/khanchandaniashish/CS131/main/Global%20YouTube%20Statistics.csv

mkdir "United States"

cat -v 'Global YouTube Statistics.csv' | grep -E '^[^,]*,[^,]*,[^,]*,[^,]*,[^,]*,[^,]*,[^,]*,United States' | awk -F',' '{ if ($5 == "Music") { print $0 } }' > ./"United States"/Music.txt

cat -v 'Global YouTube Statistics.csv' | grep -E '^[^,]*,[^,]*,[^,]*,[^,]*,[^,]*,[^,]*,[^,]*,United States' | awk -F',' '{ if ($5 == "Entertainment") { print $0 } }' > ./"United States"/Entertainment.txt

cat -v 'Global YouTube Statistics.csv' | grep -E '^[^,]*,[^,]*,[^,]*,[^,]*,[^,]*,[^,]*,[^,]*,United States' | awk -F',' '{ if ($5 == "Gaming") { print $0 } }' > ./"United States"/Gaming.txt

cat -v 'Global YouTube Statistics.csv' | grep -E '^[^,]*,[^,]*,[^,]*,[^,]*,[^,]*,[^,]*,[^,]*,United States' | awk -F',' '{ if ($5 == "Comedy") { print $0 } }' > ./"United States"/Comedy.txt

wc -l "United States"/Music.txt > ws5.txt

wc -l "United States"/Entertainment.txt >> ws5.txt

wc -l "United States"/Gaming.txt >> ws5.txt

wc -l "United States"/Comedy.txt >> ws5.txt
